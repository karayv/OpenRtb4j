        <repository>
            <id>internal_nexus_repository</id>
            <name>My releases</name>
            <url>http://build.youappi.com:8081/nexus/content/repositories/releases/</url>
        </repository>

        <!-- Publish snapshots here -->
        <snapshotRepository>
            <id>internal_nexus_repository</id>
            <name>My snapshots</name>
            <url>http://build.youappi.com:8081/nexus/content/repositories/snapshots/</url>
